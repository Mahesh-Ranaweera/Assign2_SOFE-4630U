extends dashb_layout

block dashb

    .section
        .section-header
            | Website Information
        .content-card
            form.uk-grid-small(method='POST')
                .uk-margin
                    .uk-width-1-1
                        label.text-label(for='strWebsiteName') Website Name:
                        input.uk-input(name='strWebsiteName', type='text', required)
                    .uk-margin
                        .uk-text-right
                            button.uk-button.uk-button-primary(name='btnSigin', type='submit') SIGN IN

        .section-header
            | Define web pages
        .content-card
            //-define the pages
            .uk-width-1-1
                input.uk-input.uk-width-1-2.uk-form-blank(type='text', placeholder='Enter Page Name')
                .uk-text-left
                    button.uk-button.uk-button-primary(name='btnSigin', type='submit') SIGN IN